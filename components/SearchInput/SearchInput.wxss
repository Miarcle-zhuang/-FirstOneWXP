.search_input {
  height: 90rpx;
  padding: 10rpx;
  background-color: var(--themeColor);
}
.search_input navigator {
  height: 100%;
  display: flex;
  /* 使navigator元素和里面的文字水平居中显示 */
  justify-content: center;
  /* 使navigator元素里面的文字垂直居中显示 */
  align-items: center;
  background-color: #fff;
  border-radius: 15rpx;
  color: #666;
}
